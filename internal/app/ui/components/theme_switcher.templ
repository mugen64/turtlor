package components

templ ThemeSwitcher() {
	<div class="flex items-center gap-2">
		<button data-theme="light" class="p-2 rounded bg-gray-100 dark:bg-gray-700">Light</button>
		<button data-theme="dark" class="p-2 rounded bg-gray-100 dark:bg-gray-700">Dark</button>
		<button data-theme="system" class="p-2 rounded bg-gray-100 dark:bg-gray-700">System</button>
	</div>
}
